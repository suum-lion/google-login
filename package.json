{
  "name": "ethers-typescript-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tanstack/react-query": "^4.10.3",
    "@tanstack/react-query-devtools": "^4.10.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.64",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts,.tsx . --fix",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "@likelion-campus/eslint-config",
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "@typescript-eslint/no-unsafe-call": "off"
    },
    "ignorePatterns": [
      "src/libs/**"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "prettier": "@likelion-campus/prettier-config",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@likelion-campus/eslint-config": "^0.0.1",
    "@likelion-campus/prettier-config": "^0.0.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.25.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "organize-imports-cli": "^0.10.0",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "react-app-rewire-alias": "^1.1.7",
    "react-app-rewired": "^2.2.1",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.1.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "organize-imports-cli",
      "yarn format",
      "yarn lint"
    ],
    "*.{js,css,md}": "yarn format"
  }
}
